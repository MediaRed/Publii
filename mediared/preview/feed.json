        {
            "id": "file:////home/tom/Documents/Publii/mediared/preview/entramada/index.html",
            "url": "file:////home/tom/Documents/Publii/mediared/preview/entramada/index.html",
            "title": "Encuentros",
            "summary": "Encuentros Internacionales de Mediactivismo y Comunicación Alternativa: - \"Emergências\", Rio de Janeiro,&hellip;",
